{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "moduleResolution": "node",
    "isolatedModules": false,
    "resolveJsonModule": true,
    "noEmit": true,
    "types": ["jest", "node"]
  },
  "include": ["src"]
}